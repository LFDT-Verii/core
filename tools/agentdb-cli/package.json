{
  "name": "@verii/agentdb-cli",
  "version": "0.5.0-build",
  "description": "A cli for working with agent database.",
  "repository": "https://github.com/LFDT-Verii/core",
  "main": "src/index.js",
  "bin": {
    "agentdb-cli": "src/index.js"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test node --test --test-concurrency=1 --experimental-test-module-mocks --experimental-test-coverage **/*.test.js --passWithNoTests",
    "test:ci": "cross-env NODE_ENV=test node --test --test-concurrency=1 --experimental-test-module-mocks --experimental-test-coverage --test-reporter=spec --test-reporter=junit --test-reporter-destination=stdout --test-reporter-destination=test-results.junit.xml --test-reporter=lcov --test-reporter-destination=lcov.info **/*.test.js --passWithNoTests",
    "lint": "eslint .",
    "lint:ci": "eslint . --format json >> eslint-results.json",
    "lint:fix": "eslint --fix ."
  },
  "author": "Denis Smolonski",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@verii/tests-helpers": "0.5.0-build",
    "eslint": "10.0.2",
    "eslint-config-airbnb-extended": "3.0.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-better-mutation": "2.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-watch": "8.0.0",
    "expect": "30.2.0",
    "globals": "17.3.0",
    "prettier": "3.8.1"
  },
  "dependencies": {
    "@verii/crypto": "^0.5.0-build",
    "commander": "~13.1.0",
    "lodash": "^4.17.21",
    "mongodb": "~6.21.0"
  }
}
